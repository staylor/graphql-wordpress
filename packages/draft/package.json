  "scripts": {
    "clean": "rimraf graphql-build",
    "dev": "GQL_HOST=http://localhost:8080 BABEL_ENV=app kyt dev -C kyt.dev.js",
    "graphql-dev": "BABEL_ENV=graphql nodemon --config nodemon.json --exec babel-node src/server/graphql/index.js",
    "build": "BABEL_ENV=app kyt build -C kyt.prod.js",
    "start": "forever start build/server/main.js",
    "prod": "npm run build && npm run start",
    "graphql-schema": "mkdirp graphql-build/schema && cp -R src/server/graphql/schema graphql-build",
    "graphql-babel": "babel src/server/graphql -d graphql-build",
    "graphql-build": "npm run clean && npm run graphql-schema && npm run graphql-babel",
    "graphql-start": "forever start graphql-build/index.js",
    "graphql-prod": "npm run graphql-build && npm run graphql-start",
    "schema-sdl": "babel-node src/server/graphql/build-schema.js",
    "schema-json": "apollo-codegen introspect-schema http://localhost:8080/graphql --output src/server/graphql/generated/schema.json",
    "schema": "npm run schema-sdl && npm run schema-json",
    "types": "apollo-codegen generate src/server/graphql/generated/schema.graphql --schema src/server/graphql/generated/schema.json --target flow --output src/types/flow.js",
    "prettier": "prettier-eslint \"src/**/*.js\" --ignore \"src/public/**\" --write",
    "precommit": "eslint src/",
    "media": "BABEL_ENV=graphql babel-node src/server/graphql/data/media.js"
  },
  "devDependencies": {
    "apollo-codegen": "^0.18.3",
    "babel-cli": "^6.26.0",
